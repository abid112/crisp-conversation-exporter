{
  "version": 2,
  "functions": {
    "api/export-conversations.js": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/api/export-conversations",
      "dest": "/api/export-conversations.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
} 